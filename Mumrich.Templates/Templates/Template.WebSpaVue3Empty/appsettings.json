{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "SinglePageApps": {
    "/": {
      "DevServerAddress": "http://localhost:3000/",
      "SpaRootPath": "ClientApp",
      "StartCommand": "yarn dev",
      "Regex": "dev server running at:",
      "Bundler": "ViteJs",
      "Environment": {
        "HMR_PORT": "%ASPNETCORE_HTTPS_PORT%",
        "HMR_PROTOCOL": "wss"
      }
    }
  }
}